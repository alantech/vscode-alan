{
    "name": "alan-lang",
    "displayName": "Alan",
    "description": "Syntax highlighting for Alan Lang",
    "version": "0.1.1",
    "license": "Apache-2.0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/logo.png",
    "repository": "https://github.com/alantech/vscode-alan",
    "publisher": "alantech",
    "contributes": {
        "languages": [
            {
                "id": "alan",
                "aliases": [
                    "Alan",
                    "alan"
                ],
                "extensions": [
                    ".ln",
                    ".lnn"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "alan--",
                "aliases": [
                    "Alan--",
                    "alan--"
                ],
                "extensions": [
                    ".amm"
                ],
                "configuration": "./amm-language-configuration.json"
            },
            {
                "id": "alan graphcode",
                "aliases": [
                    "Alan Graphcode",
                    "Alan GraphCode",
                    "alan graphcode"
                ],
                "extensions": [
                  ".aga"
                ]
            }
        ],
        "grammars": [
            {
                "language": "alan",
                "scopeName": "source.ln",
                "path": "./syntaxes/alan.tmLanguage.json"
            },
            {
                "language": "alan--",
                "scopeName": "source.amm",
                "path": "./syntaxes/amm.tmLanguage.json"
            },
            {
                "language": "alan graphcode",
                "scopeName": "source.aga",
                "path": "./syntaxes/aga.tmLanguage.json"
            }
        ]
    }
}
