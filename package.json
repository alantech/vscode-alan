{
    "name": "alan-lang",
    "displayName": "Alan",
    "description": "Syntax highlighting for Alan Lang",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/logo.png",
    "repository": "https://github.com/alantech/vscode-alan",
    "publisher": "alantech",
    "contributes": {
        "languages": [
            {
                "id": "alan",
                "aliases": [
                    "Alan",
                    "alan"
                ],
                "extensions": [
                    ".ln"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "alan",
                "scopeName": "source.ln",
                "path": "./syntaxes/alan.tmLanguage.json"
            }
        ]
    }
}
